<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Height of the battery icon in the status bar. -->
    <dimen name="status_bar_battery_icon_height">14.0dp</dimen>

    <!-- Width of the battery icon in the status bar. The battery drawable assumes a 12x20 canvas,
    so the width of the icon should be 13.0dp * (12.0 / 20.0) -->
    <dimen name="status_bar_battery_icon_width">13.0dp</dimen>

    <!-- extra height to add on qs status bar heigth if header enabled -->
    <dimen name="qs_header_image_offset">25dp</dimen>

    <!-- padding between the DateView and qs data usage header -->
    <dimen name="data_usage_padding_start">0dp</dimen>
</resources>
